{
    "out": "./docs",
    "entryPoints": ["./src/**/*.ts"],
    "exclude": [
        "./src/**/*.test.ts",
        "./src/**/_module.ts",
        "./src/**/_module-exports.ts"
    ],
    "navigation": {
        "includeGroups": true,
        "includeFolders": true
    },
    "projectDocuments": ["src/**/*.md"],
    "categorizeByGroup": true,
    "includeVersion": true,
    "excludeInternal": true,
    "excludePrivate": true,
    "excludeProtected": true,
    "cleanOutputDir": true,
    "githubPages": true,
    "plugin": ["typedoc-plugin-merge-modules"],
    "mergeModulesMergeMode": "module"
}
